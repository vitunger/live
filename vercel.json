{
  "headers": [
    {
      "source": "/portal/(.*).js",
      "headers": [
        { "key": "Content-Type", "value": "application/javascript; charset=utf-8" },
        { "key": "Cache-Control", "value": "public, max-age=3600" }
      ]
    }
  ],
  "rewrites": [
    { "source": "/portal/:path*", "destination": "/portal/:path*" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
